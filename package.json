{
	"name": "fantasytennis",
	"version": "1.0.0",
	"author": "Alex Cadigan",
	"description": "Play fantasy tennis for free!",
	"keywords": [
		"Free",
		"Fantasy",
		"Tennis"
	],
	"license": "MIT",
	"homepage": ".",
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexCadigan/FantasyTennis.git"
	},
	"bugs": {
		"url": "https://github.com/AlexCadigan/FantasyTennis/issues"
	},
	"scripts": {
		"build": "react-scripts build",
		"start": "react-scripts start",
		"start:dev": "concurrently --kill-others \"npm run startServer:dev\" \"npm run startClient:dev\"",
		"startServer:dev": "npx nodemon",
		"startClient:dev": "npm run start --prefix client",
		"lint": "npm run prettier && npm run eslint",
		"prettier": "npx prettier --write .",
		"eslint": "npx eslint .",
		"test": "npm run test --prefix tests"
	},
	"dependencies": {
		"@aws-amplify/ui-react": "^6.0.6",
		"aws-amplify": "^6.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"concurrently": "^7.6.0",
		"eslint": "^8.24.0",
		"eslint-plugin-jest": "^27.0.4",
		"eslint-plugin-jsdoc": "^39.3.6",
		"eslint-plugin-react": "^7.31.8",
		"husky": "^4.3.8",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4",
		"@types/react-dom": "^18.0.10",
		"react-scripts": "^5.0.1",
		"web-vitals": "^3.1.0"
	}
}
