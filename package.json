{
	"name": "fantasytennis",
	"version": "1.0.0",
	"author": "Alex Cadigan",
	"description": "Play fantasy tennis for free!",
	"keywords": [
		"Free",
		"Fantasy",
		"Tennis"
	],
	"license": "MIT",
	"main": "app.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexCadigan/FantasyTennis.git"
	},
	"homepage": "https://github.com/AlexCadigan/FantasyTennis#readme",
	"bugs": {
		"url": "https://github.com/AlexCadigan/FantasyTennis/issues"
	},
	"scripts": {
		"nodemon": "npx nodemon",
		"devStart": "node dist/server/app.js",
		"predevStart": "npm run devBuild",
		"devBuild": "tsc",
		"predevBuild": "npm run devPrettier && npm run devEslint",
		"devPrettier": "npx prettier --write **/*.{json,ts}",
		"devEslint": "npx eslint **/*.ts",
		"test": "jest"
	},
	"dependencies": {
		"cookie-parser": "^1.4.6",
		"express": "^4.18.1",
		"http-errors": "^2.0.0",
		"morgan": "^1.10.0",
		"pug": "^3.0.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.3",
		"@types/express": "^4.17.14",
		"@types/http-errors": "^1.8.2",
		"@types/jest": "^29.1.0",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"eslint": "^8.24.0",
		"eslint-plugin-jsdoc": "^39.3.6",
		"eslint-plugin-react": "^7.31.8",
		"husky": "^4.3.8",
		"jest": "^29.1.1",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"supertest": "^6.2.4",
		"typescript": "^4.8.4"
	}
}
